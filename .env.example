# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_KEY=your_service_key_here
DATABASE_URL=postgresql://postgres.your-project:your-password@aws-region.pooler.supabase.com:5432/postgres

# Server Configuration
PORT=3001
HOST=0.0.0.0
NODE_ENV=production

# Upload Configuration
MAX_FILE_SIZE=52428800
ALLOWED_MIME_TYPES=application/json,application/jsonl,text/plain,text/csv,application/x-jsonlines,text/x-json-lines,application/x-ndjson,text/jsonl

# Rate Limiting (프로덕션 환경만 적용)
RATE_LIMIT_MAX=1000
RATE_LIMIT_WINDOW=300000

# Claude API (AI 요약 생성용)
CLAUDE_API_KEY=your_claude_api_key_here